{
	"name": "DQS",
	"displayName": "dqs",
	"description": "依赖快速排序 Dependencies Quickly Sort",
	"publisher": "PuddingAndBall",
	"version": "2.0.2",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.jpeg",
	"activationEvents": [
		"onCommand:dqs.sort"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dqs.sort",
				"title": "dqs.sort"
			}
		],
		"keybindings": [
			{
				"command": "dqs.sort",
				"key": "ctrl+alt+q",
				"mac": "ctrl+shift+q"
			}
		],
		"configuration": {
			"properties": {
				"DQS.formatAfterSort": {
					"type": "boolean",
					"default": true,
					"description": "auto format code with prettier after sorting dependencies"
				},
				"DQS.sortOnSaveEnabled": {
					"type": "boolean",
					"default": true,
					"description": "auto sort dependencies on save"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"prettier": "^3.3.2",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.3",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"axios": "^0.24.0"
	}
}